<template name="albun-list">
	<view class="big-photo-list">
		<view class="big-photo-item" wx:for="{{templateData}}">
			<image
			 class="photo"
			 src="{{item.imgUrl}}"
			 mode="scaleToFill"
			 lazy-load
			/>
			<text class="music-name">{{item.musicName}}</text>
			<text class="music-author">
                {{item.author}}
            </text>
		</view>
	</view>
</template>

